<h1>Log in</h1>
<%= flash[:warning] unless flash.empty? %>
<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Log in" %>

<% end %>
<%= link_to "Sign up now!", "#" %>
